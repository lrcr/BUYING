<div class="modal-header">
  <button type="button" class="close" data-dismiss="modal" aria-label="Close">
    <span aria-hidden="true">&times;</span>
  </button>
  <h4 class="modal-title">응답 내역</h4>
</div>
<div class="modal-body">
  <form class="form-horizontal">
    <%for(var i = 0 ; i<data.length; i++){ %>
    <fieldset>

              <div class="form-group">
                <label class="col-lg-2 control-label">응답 일시</label>
                <div class="col-lg-10">
                  <input type="text" class="form-control" value="<%=data[i].date%>" readonly>
                </div>
              </div>


                  <div class="form-group">
                    <label class="col-lg-2 control-label">모델명</label>
                    <div class="col-lg-10">
                      <input type="text" class="form-control" value="<%=data[i].name%>" readonly>
                    </div>
                  </div>


                  <div class="form-group">
                    <label class="col-lg-2 control-label">브랜드</label>
                    <div class="col-lg-10">
                      <input type="text" class="form-control" value="<%=data[i].brand%>" readonly>
                    </div>
                  </div>


                <div class="form-group">
                  <label class="col-lg-2 control-label">상태등급</label>
                  <div class="col-lg-10">
                    <input type="text" class="form-control" value="<%=data[i].grade%>" readonly>
                  </div>
                 </div>




             <div class="form-group">
               <label class="col-lg-2 control-label">가격</label>
               <div class="col-lg-10">
                 <input type="number" class="form-control" value="<%=data[i].price%>" readonly>
               </div>
             </div>


             <div class="form-group">
               <label class="col-lg-2 control-label">구성품</label>
               <div class="col-lg-10">
                 <input type="text" class="form-control" value="<%=data[i].component%>" readonly>
               </div>
             </div>



                  <div class="form-group">
                    <label for="textArea" class="col-lg-2 control-label">Buyer 한마디</label>
                    <div class="col-lg-10">
                      <textarea class="form-control" rows="3" id="mention" readonly><%=data[i].mention%></textarea>
                    </div>
                  </div>

                  <div class="form-group">
                    <label class="col-lg-2 control-label">거래예정일자</label>
                    <div class="col-lg-10">
                      <input type="text" class="form-control" value="<%=data[i].trade_date%>" readonly>
                    </div>
                  </div>



                  <div class="form-group">
                    <label class="col-lg-2 control-label">대표사진</label>
                    <div class="col-lg-10">
                      <img src="<%=data[i].img[0]%>" style="width:100%; height : 350px; ">
                    </div>
                    <%for(var j = 1; j< data[i].img.length; j++){%>
                    <label class="col-lg-2 control-label">사진</label>
                    <div class="col-lg-10">
                      <img src="<%=data[i].img[j]%>" style="width:100%; height : 350px; ">
                    </div>
                    <%}%>
                  </div>



        </fieldset>
        <%}%>
    <div class="modal-footer">
      <input type="button" class="btn btn-default" data-dismiss="modal" value="닫기" />
    </div>
  </form>
</div>