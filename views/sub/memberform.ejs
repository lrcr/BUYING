					    			<td class="mem_name"><a href="#" data-toggle="modal" data-target="#member_trade"><%=results[i].mem_name%></a></td>
					    			<td class="mem_phone"><%=results[i].mem_phone%></td>
					    			<td class="service_agr"><%=results[i].service_agr%></td>
